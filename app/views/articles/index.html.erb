<h1>Showing all Articles</h1>


<table>
    <tr>
        <th>Title</th>
        <th>Description</th>
    </tr>

    <%@articles.each do |article|%>
        <tr>
            <td><%= article.title%></td>
            <td><%=article.description%> </td>
        
        </tr>
         
    <%end%>
</table>